                <script>
                        var token = "bf5d11d1c25fc72a8704579190ab64d3661fe6d9";
                        var app = angular.module('ppdrvDls', []);
                        var theUrl =
                        app.controller('ppdrvDlsCtrl', function($scope, $http){
                                $http({
                                        method : "GET",
                                        url : .get("https://api.pipedrive.com/v1/deals?api_token=").then(function(response){
                                        $scope.myData = response.data.data;
                                });
                        });
